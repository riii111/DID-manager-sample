default_job = "run"

[jobs.run]
command = ["cargo", "run", "--bin", "miax-agent"]
need_stdout = true

[jobs.check]
command = ["cargo", "check", "--all-features"]
need_stdout = false

[jobs.test]
command = ["cargo", "test", "--all-features"]
need_stdout = true

[jobs.clippy]
command = ["cargo", "clippy", "--all-features"]
need_stdout = true